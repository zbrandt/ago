<nav class="bg-purple-900 shadow-lg">
  <div class="px-4 sm:px-6 lg:px-8">
    <div class="flex justify-between items-center h-20">
      <div class="flex flex-row items-center">
        {/* Logo/Icon */}
        <div class="flex-shrink-0">
          <a href="/ago" class="flex items-center">
            <img class="h-10 w-10" src="/ago/favicon.svg" alt="Fraternity Logo" />
            <span class="ml-2 text-2xl font-semibold text-white">Fraternity</span>
          </a>
        </div>
        
        {/* Navigation Links */}
        <div class="hidden md:block ml-10">
          <div class="flex items-center space-x-8">
            <a href="/housing/" 
               class="text-white hover:bg-purple-800 px-4 py-3 rounded-md text-lg font-semibold transition duration-150 ease-in-out">
              About
            </a>
            <a href="/about/" 
               class="text-white hover:bg-purple-800 px-4 py-3 rounded-md text-lg font-semibold transition duration-150 ease-in-out">
